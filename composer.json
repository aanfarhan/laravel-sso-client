{
    "name": "mdigi/laravel-sso-client",
    "description": "Laravel package for OAuth client using Laravel Socialite with LaravelPassport driver",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mdigi\\LaravelSsoClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MDIGI",
            "email": "info@mdigi.co.id"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "laravel/socialite": "^5.0",
        "socialiteproviders/laravelpassport": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.6|^10.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mdigi\\LaravelSsoClient\\SsoClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}