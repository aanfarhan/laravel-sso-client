{
    "name": "omniglies/laravel-sso-client",
    "description": "Laravel package for OAuth client using Laravel Socialite with LaravelPassport driver",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omniglies\\LaravelSsoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omniglies\\LaravelSsoClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "aanfarhan",
            "email": "abaz.nurfarhana@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "laravel/socialite": "^5.0",
        "socialiteproviders/laravelpassport": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.5|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omniglies\\LaravelSsoClient\\SsoClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}